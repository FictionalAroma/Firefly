; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Firefly"
config/version="0.1"
run/main_scene="res://node_2d.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=1280
window/size/viewport_height=800
window/energy_saving/keep_screen_on=false
window/stretch/mode="viewport"

[editor]

naming/node_name_num_separator=2
naming/node_name_casing=1
naming/default_signal_callback_name="on{node_name}{signal_name}"
naming/default_signal_callback_to_self_name="on{signal_name}"
naming/scene_name_casing=1

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"

[xr]

openxr/extensions/hand_tracking=false
